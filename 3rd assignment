9)2sum
import collections
file = open('rosalin.txt')
lines = file.readlines()
fline = list(map(int, lines[0].split()))
k=fline[0]
m=int(fline[1])
for i in range(1, k+1):
    d={}
    lst = list(map(int, lines[i].split()))
    sentinella=False
    for j in range(len(lst)):
        if -lst[j] in d.keys() and abs(lst[j])<=m:
            print (d.get(-lst[j])+1,(int(j)+1))
            sentinella=True
    
            break  
        
        else:
            d[lst[j]] = j

    if sentinella==False:
        print(-1)
        
10)par
file=open('rosalind_par.txt')
line=file.readlines()
n=int(line[0])
arr=list(map(int,line[1].split()))
lst=[]
i = arr[0]
arr1 = []
arr2 = []
for x in arr[1:]:
    if x <= i:
        arr1.append(x)
for x in arr[1:]:
    if x > i: 
        arr2.append(x)
print(*arr1+[i]+arr2)
    
11)3sum
def two_sum(a, target2=0):
    tmp_dict = {}
    for i in range(len(a)):
        if a[i] in tmp_dict:
            return (tmp_dict[a[i]]+1, i+1)
        else:
            tmp_dict[target2-a[i]] = i
    return -1

def three_sum(a, target3=0):
    for i in range(len(a)):
        res = two_sum(a[i+1:], target2=target3-a[i])
        if res != -1:
            print(i+1, i+1+res[0], i+1+res[1])
            return (i+1, i+1+res[0], i+1+res[1])
    print(-1)
    return -1


with open("rosalind_3sum.txt", "r") as f:
    k,n = map(int, f.readline().strip().split())
    A = [[int(i) for i in line.strip().split()] for line in f]
    for i in range(k):
        r = three_sum(A[i]) 
 
12)par3
file=open("rosalind_3sum.txt",'r')
lines = file.readlines()
l = list(map(int, lines[0].split()))
k,n=int(l[0]),int(l[1])
for i in range(1, k+1):
    arr = list(map(int, lines[i].split()))
    sentinella=-1
    for j in range(n):
        dict_sol={} 
        a=-arr[j]
        for f in range(j+1,n): 
            if arr[f] in dict_sol:
                b,c=dict_sol.get(arr[f])+1,f+1
                sentinella=0
                break
            else:
                dict_sol[a-arr[f]]=f
        if sentinella==0:
            break

    if sentinella==-1:
        print("-1") 
    else:
        print("{} {} {} ".format(j+1,b,c))
 
  
