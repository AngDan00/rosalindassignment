lines = file.readlines()
n = int(lines[0])
lst1 = list(map(int, lines[1].split()))
m=int(lines[2])
lst2 = list(map(int, lines[3].split()))
a=sorted(lst1+lst2)
for i in a:
    print(i,end=' ')

